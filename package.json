{
  "name": "firstlab",
  "version": "1.0.0",
  "description": "A tinny component lib",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint src --max-warnings=0",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Morpa/first-lib.git"
  },
  "keywords": [
    "ui"
  ],
  "author": "Morpa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Morpa/first-lib/issues"
  },
  "homepage": "https://github.com/Morpa/first-lib#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@rollup/plugin-commonjs": "^23.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@storybook/addon-essentials": "7.0.4",
    "@storybook/builder-webpack5": "^7.0.2",
    "@storybook/manager-webpack5": "^6.4.17",
    "@storybook/react": "6.5.10",
    "@swc/core": "^1.2.208",
    "@swc/jest": "^0.2.15",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.1",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/styled-components": "^5.1.22",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-loader": "^8.2.5",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.0.1",
    "jest-styled-components": "^7.0.8",
    "lint-staged": "^13.0.3",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.75.7",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "styled-components": "^5.3.5",
    "typescript": "^4.5.5",
    "webpack": "5"
  }
}
